<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="../public/order.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css">
      
      <link href='https://fonts.googleapis.com/css?family=Roboto' rel='stylesheet'>
    <title>Document</title>
</head>
<body>
    <header>
      <nav class="sec-1">
        <div class="p-1">
          <!--  <a href="../index"><i class="fa fa-book"></i></a>-->
          <a href="../index">
            <h3>BOOK<span>STORE</span></h3>
          </a>
        </div>
        <div class="p-2">
          <ul>
            <a href="../index">
              <li>Home</li>
            </a>
            <a href="../shop">
              <li>Shop</li>
            </a>
            <a href="../order">
              <li>Order</li>
            </a>
            <a href="../feedback">
              <li>Feedback</li>
            </a>
          </ul>
        </div>
        <div class="p-3">
           <a href="../profile" id="username">{{user.u_name}}</a>
         

         
          <a href="../cart"><i class="fa fa-shopping-cart"></i></a>
        </div>
      </nav>
    </header>


    <div class="order">
    {{#each books}}
    <div class="book-1" id="book-1">
         
           

            <h1>ORDER_ID: {{this.or_id}}</h1>
            <h1>NAME: {{this.u_name}}</h1>
            <h1>ADDRESS: {{this.u_address}}</h1>
            <h1>PINCODE: {{this.pincode}}</h1>
            <h1>STATE: {{this.state}}</h1>
            <h1>CITY: {{this.u_city}}</h1>
            <h1>ORDER_DATE: {{this.order_date}}</h1>
            <h1>ORDER_REC_DATE: {{this.order_rec}}</h1>
            <h1>TOTAL RS.: {{this.total_amount}}</h1>
            <h1>BOOK_ID: {{this.b_id}}</h1>
             <h1>BOOK_NAME: {{this.b_name}}</h1>
            <h1 id="mode">PAYMENT_MODE: {{this.payment_mode}}</h1>
              <h1>STATUS: {{this.status}}</h1>
            
     </div>
    {{/each}}
  </div>

</body>
</html>